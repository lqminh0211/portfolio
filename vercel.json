{
    "version": 2,
    "builds": [
      { 
        "src": "/api/index.php",
        "use": "vercel-php@0.6.0"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ],
    "env": {
      "APP_NAME": "Vercel Laravel",
      "APP_ENV": "production",
      "APP_DEBUG": "false",
      "APP_URL": "https://php-laravel.vercel.app",
      "VERCEL_DEMO_MODE": "true",
      "CACHE_DRIVER": "array",
      "LOG_CHANNEL": "stderr",
      "SESSION_DRIVER": "array",
      "VIEW_COMPILED_PATH": "/tmp/views",
      "SSR_TEMP_PATH": "/tmp/ssr",
      "NODE_PATH": "node"
    }
  }
  